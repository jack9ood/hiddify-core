{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 1080
    }
  ],
  "outbounds": [
    {
      "type": "uap",
      "tag": "uap-reality-test",
      "server": "YOUR_SERVER_IP",
      "server_port": 443,
      "uuid": "YOUR_UUID",
      "flow": "",
      "tls": {
        "enabled": true,
        "server_name": "www.microsoft.com",
        "insecure": false,
        "reality": {
          "enabled": true,
          "public_key": "YOUR_PUBLIC_KEY",
          "short_id": "YOUR_SHORT_ID"
        }
      },
      "transport": {
        "type": "tcp"
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": "mixed-in",
        "outbound": "uap-reality-test"
      }
    ],
    "final": "direct"
  }
}

